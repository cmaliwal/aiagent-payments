# Example environment variables for AI Agent Payments SDK

# Stripe Configuration
STRIPE_API_KEY=sk_test_yourkeyhere
STRIPE_WEBHOOK_SECRET=whsec_yoursecrethere

# PayPal Configuration
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret
PAYPAL_SANDBOX=true

# Database Configuration
DATABASE_URL=sqlite:///payments.db

# Crypto Wallet
CRYPTO_WALLET_ADDRESS=0xYourWalletAddress
CRYPTO_NETWORK=ethereum

# Logging
AIAGENT_LOG_LEVEL=INFO
AIAGENT_LOG_FILE=aiagent_payments.log
AIAGENT_LOG_COLORS=true 